{
    "java.project.sourcePaths": [
        "src/main/java",
        "src/main/resources"
    ],
    "java.project.outputPath": "target/classes",
    "java.project.referencedLibraries": [
        "lib/**/*.jar"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.class": true
    },
    "java.configuration.updateBuildConfiguration": "interactive"
}